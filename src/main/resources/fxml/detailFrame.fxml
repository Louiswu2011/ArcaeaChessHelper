               AnchorPane.leftAnchor="74.0"/>
        <Label layoutX="14.0" layoutY="85.0" text="Song: " AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="85.0"/>
        <Label fx:id="idholder" layoutX="54.0" layoutY="85.0" text="SongIDHolder" AnchorPane.leftAnchor="54.0"
               AnchorPane.topAnchor="85.0"/>
        <Label layoutX="14.0" layoutY="108.0" text="Pure: " AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="108.0"/>
        <Label fx:id="pureholder" layoutX="50.0" layoutY="108.0" text="PureHolder" AnchorPane.leftAnchor="50.0"/>
        <Label layoutX="14.0" layoutY="130.0" text="Far:"/>
