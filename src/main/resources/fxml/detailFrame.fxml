        <Label fx:id="lostholder" layoutX="49.0" layoutY="152.0" text="LostHolder"/>
        <Label layoutX="14.0" layoutY="175.0" text="B.Potential: "/>
        <Label fx:id="bpholder" layoutX="85.0" layoutY="175.0" text="BasePotentialHolder"/>
        <Label fx:id="typeholder" alignment="CENTER" layoutY="250.0" prefHeight="15.0" prefWidth="250.0"
