<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="310.0" prefWidth="250.0"
            xmlns="http://javafx.com/javafx/8.0.141">
    <children>
        <Label layoutX="14.0" layoutY="18.0" text="Player: " AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="18.0"/>
        <Label fx:id="nameholder" layoutX="57.0" layoutY="18.0" text="PlayerNameHolder" AnchorPane.leftAnchor="57.0"
               AnchorPane.topAnchor="18.0"/>
        <Button fx:id="close" layoutX="67.0" layoutY="267.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="66.0"
