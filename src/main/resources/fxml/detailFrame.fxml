        <Label fx:id="timeholder" layoutX="14.0" layoutY="221.0" text="Played on"/>
        <Label fx:id="lockstate" layoutX="170.0" layoutY="130.0" text="Skill Locked" textFill="#ff1313"
               AnchorPane.rightAnchor="12.0"/>
        <ProgressBar fx:id="healthbar" layoutX="63.0" layoutY="198.0" prefHeight="18.0" prefWidth="141.0"
                     progress="1.0"/>
        <Label layoutX="14.0" layoutY="198.0" text="Health:"/>
        <Label fx:id="healthpoint" layoutX="209.0" layoutY="199.0" text="100%" AnchorPane.rightAnchor="9.0"/>
